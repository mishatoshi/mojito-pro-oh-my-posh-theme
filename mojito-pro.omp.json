{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#70DB86",
          "style": "plain",
          "template": "\u250c ",
          "type": "text"
        },
        {
          "foreground": "#F7776E",
          "style": "plain",
          "template": "\uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#A3F5B5",
          "style": "plain",
          "template": "<#70DB86></>{{ if .WSL }}WSL at {{ end }}{{.Icon}}<#70DB86> </>",
          "type": "os"
        },
        {
          "foreground": "#A3F5B5",
          "style": "plain",
          "template": "<#70DB86></>{{ .UserName }}<#70DB86> </>",
          "type": "session"
        },
        {
          "foreground": "#A3F5B5",
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_template": "<#70DB86> \u00bb </>",
            "home_icon": "\ueb06",
            "style": "agnoster"
          },
          "style": "plain",
          "template": "<#70DB86>:: </>\ue5ff {{ .Path }}<#70DB86> </>",
          "type": "path"
        },
        {
          "foreground": "#FFF066",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#70DB86>:: </>{{ .HEAD }}{{ if .Staging.Changed }}<#70DB86> \uf044 {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#6CBAF9> \uf044 {{ .Working.String }}</>{{ end }}<#70DB86> </>",
          "type": "git"
        },
        {
          "foreground": "#B6E085",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#70DB86>:: </>\ue781 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#70DB86> </>",
          "type": "node"
        },
        {
          "foreground": "#6CBAF9",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#70DB86>:: </>\ue73d {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#70DB86> </>",
          "type": "php"
        },
        {
          "foreground": "#FFF066",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": "<#70DB86>:: </>\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#70DB86> </>",
          "type": "python"
        },
        {
          "foreground": "#F7776E",
          "style": "plain",
          "template": "<#70DB86>:: </>\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#70DB86> </>",
          "type": "java"
        },
        {
          "foreground": "#71E5F4",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#70DB86>:: </>î˜§ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#70DB86> </>",
          "type": "go"
        },
        {
          "foreground": "#FFAD66",
          "style": "plain",
          "template": "<#70DB86>:: </>\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#70DB86> </>",
          "type": "rust"
        },
        {
          "foreground": "#A3F5B5",
          "properties": {
            "time_format": "02/01/06 15:04"
          },
          "style": "plain",
          "template": "<#70DB86>:: </>\uf073 {{ .CurrentDate | date .Format }}<#70DB86> </>",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#A3F5B5",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "<#70DB86>\u2514 </>\ueba2 {{ .FormattedMs }} <#70DB86></>",
          "type": "executiontime"
        },
        {
          "foreground": "#70DB86",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#F7776E{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f\u276f",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}[root] {{end}}{{.Shell}} in <{{.Folder}}>",
  "final_space": true,
  "version": 3
}
